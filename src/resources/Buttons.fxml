<?xml version="1.0" encoding="UTF-8"?>
<!-- Buttons.fxml -->
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.CheckBox?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="main.JavaFxController.ButtonsController"
      stylesheets="" spacing="10">
    <HBox spacing="10">
        <Button text="RUN"  fx:id="btnRun" />
        <Button text="STEP" fx:id="btnStep"/>
        <Button text="STOP" fx:id="btnStop" />
        <Button text="MCLR" fx:id="btnMCLR"/>
        <CheckBox
                fx:id="activateWatchdogCheckbox"
                text="Activate watchdog"/>
    </HBox>
</VBox>
